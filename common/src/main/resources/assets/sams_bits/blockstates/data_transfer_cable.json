{
  "multipart": [
    {
      "apply": {
        "model": "sams_bits:block/data_transfer_cable/post"
      }
    },
    {
      "apply": {
        "model": "sams_bits:block/data_transfer_cable/vertical",
        "uvlock": true
      },
      "when": {
        "vertical": "true"
      }
    },
    {
      "apply": {
        "model": "sams_bits:block/data_transfer_cable/side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "sams_bits:block/data_transfer_cable/side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "sams_bits:block/data_transfer_cable/side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "sams_bits:block/data_transfer_cable/side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    }
  ]
}